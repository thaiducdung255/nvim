{
   "suggest.noselect": true,
   "suggest.virtualText": true,
   "suggest.completionItemKindLabels": {
      "text": "󰉿",
      "method": "󰆧",
      "function": "󰊕",
      "constructor": "",
      "field": "󰜢",
      "variable": "󰀫",
      "class": "󰠱",
      "interface": "",
      "module": "",
      "property": "󰜢",
      "unit": "󰑭",
      "value": "󰎠",
      "enum": "",
      "keyword": "󰌋",
      "snippet": "",
      "color": "󰏘",
      "file": "󰈙",
      "reference": "󰈇",
      "folder": "󰉋",
      "enumMember": "",
      "constant": "󰏿",
      "struct": "󰙅",
      "event": "",
      "operator": "󰆕",
      "typeParameter": "󰗴",
      "default": ""
   },
   "snippets.textmateSnippetsRoots": [
      "~/.config/nvim/snippets"
   ],
   "typescript.preferences.includePackageJsonAutoImports": "off",
   "diagnostic.virtualText": true,
   "tsserver.maxTsServerMemory": 8096,
   "tsserver.log": "off",
   "typescript.format.enable": false,
   "deno.lint": true,
   "codeLens.enable": true,
   "colors.enable": true,
   "list.nextKeymap": "<C-n>",
   "list.previousKeymap": "<C-e>",
   "list.floatPreview": true,
   "coc.preferences.formatOnSave": true,
   "eslint.autoFixOnSave": true,
   "outline.autoPreview": true,
   "snippets.autoTrigger": true,
   "coc.source.db.priority": 10,
   "coc.source.buffer.priority": 30,
   "coc.source.file.priority": 20,
   "coc.source.around.priority": 40,
   "languageserver": {
      "graphql": {
         "command": "graphql-lsp",
         "args": [
            "server",
            "-m",
            "stream"
         ],
         "filetypes": [
            "graphql",
            "typescript",
            "typescriptreact",
         ]
      },
      "sql": {
         "command": "sql-language-server",
         "args": [
            "up",
            "--method",
            "node-ipc"
         ],
         "filetypes": [
            "sql",
            "mysql"
         ]
      }
   },
   "coc.preferences.extensionUpdateCheck": "daily"
}
