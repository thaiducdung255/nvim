{
   "require_module": {
      "prefix": ["rem"],
      "body": "const $1 = require('$1')"
   },
   "require_partial": {
      "prefix": ["rep"],
      "body": "const { $2 } = require('$1')"
   },
   "function": {
      "prefix": ["fn"],
      "body": [
         "function $1() {",
         "\t$2",
         "}"
      ]
   },
   "arrow_function_compact": {
      "prefix": ["af"],
      "body": [
         "($1) => $2"
      ]
   },
   "arrow_function_full": {
      "prefix": ["aff"],
      "body": [
         "($1) => {",
         "\t$2",
         "}"
      ]
   },
   "module.exports": {
      "prefix": ["me"],
      "body": "module.exports = {$1}"
   }
}
